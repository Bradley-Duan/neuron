{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": false,
    "noEmit": true,
    "jsx": "preserve",
    "plugins": [{
      "name": "typescript-tslint-plugin"
    }],
    "typeRoots": ["./types", "node_modules/@types"]
  },
  "include": [
    "src"
  ]
}
